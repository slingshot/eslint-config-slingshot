        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^5.2.0"
    },
    "author": "Sanil Chawla <sanil@slingshot.fm>",
    "license": "Apache-2.0",
